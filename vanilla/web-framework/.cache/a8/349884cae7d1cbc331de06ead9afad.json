fragment: DocumentFragment): void {\r\n    const eventsMap = this.eventsMap();\r\n    for (let eventKey in eventsMap) {\r\n      const [event, tag] = eventKey.split(\":\");\r\n      fragment\r\n        .querySelectorAll(tag)\r\n        .forEach((e) => e.addEventListener(event, eventsMap[eventKey]));\r\n    }\r\n  }\r\n\r\n  mapRegions(fragment: DocumentFragment): void {\r\n    const regionsMap = this.regionsMap();\r\n\r\n    for (let key in regionsMap) {\r\n      const selector = regionsMap[key];\r\n      const element = fragment.querySelector(selector);\r\n      if (element) {\r\n        this.regions[key] = element;\r\n      }\r\n    }\r\n  }\r\n\r\n  onRender(): void {}\r\n\r\n  render(): void {\r\n    this.parent.innerHTML = \"\"; //clears previous rendition before new render\r\n    const templateElement = document.createElement(\"template\");\r\n    templateElement.innerHTML = this.template();\r\n    this.bindEvents(templateElement.content);\r\n    this.mapRegions(templateElement.content);\r\n    this.onRender();\r\n    this.parent.append(templateElement.content);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"dd4516e77495c046657b549aa4b2eec0","cacheData":{"env":{}}}